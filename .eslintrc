{
   "parser": "babel-eslint",
   "env": {
      "es6": true,
      "browser": true,
      "node": true
   },
   "rules": {
      "indent":"off",
      "linebreak-style":"off",
      "arrow-parens":"off",
      "require-jsdoc":"off",
      "no-trailing-spaces":"off",
      "max-len":"off",
      "operator-linebreak":"off"
   },
   "extends": ["eslint:recommended", "google"]
}